{\n            setLoading(true);\n            const response = await fetch('https://fakestoreapi.com/products');\n            console.log(response,\"saipriya\")\n            if(componentMounted){\n                setData(await response.clone().json());\n                setFilter(await response.json());\n            \n                setLoading(false);\n                console.log(filter);\n                console.log(response,\"saipriya\")\n            }\n            return()=>{\n               componentMounted = false; \n            }\n        }\n\n        getProducts();\n    },[]);\n\n    const Loading = ()=>{\n        return (\n            <>\n                <div className=\"col-md-3\">\n                    <Skeleton height={350}/>\n                </div>\n                <div className=\"col-md-3\">\n                    <Skeleton height={350}/>\n                </div>\n                <div className=\"col-md-3\">\n                    <Skeleton height={350}/>\n                </div>\n                <div className=\"col-md-3\">\n                    <Skeleton height={350}/>\n                </div>\n            </>\n        );\n    };\n\n    const filterProduct = (cat) => {\n        const updatedList = data.filter((x)=> x.category === cat);\n        setFilter(updatedList)\n    }\n\n    const ShowProducts = ()=>{\n        return(\n            <>\n                <div className=\"buttons d-flex justify-content-center mb-5 pb-5\">\n                    <button className=\"btn btn-outline-dark me-2\" onClick={()=> setFilter(data)}>All</button>\n                    <button className=\"btn btn-outline-dark me-2\" onClick={()=> filterProduct(\"men's clothing\")} >Men's Clothing</button>\n                    <button className=\"btn btn-outline-dark me-2\" onClick={()=> filterProduct(\"women's clothing\")} >Women's Clothing</button>\n                    <button className=\"btn btn-outline-dark me-2\" onClick={()=> filterProduct(\"jewelery\")} >Jewelery </button>\n                    <button className=\"btn btn-outline-dark me-2\" onClick={()=> filterProduct(\"electronics\")}>Electronic </button>\n                </div>\n                {filter.map((product)=>{\n                    return (\n                        <>\n                            <div className = \"col-md-3 mb-4\">\n                                <div className = \"card h-100 text-center p-4\" key = {product.id} >\n                                    <img src={product.image} className=\"card-img-top\" alt= {product.title}  height=\"250px\"/>\n                                    <div className = \"card-body\">\n                                        <h5 className = \"card-title mb-0\">{product.title.substring(0,12)}...</h5>\n                                        <p className = \"card-text lead fw-bold\">${product.price}</p>\n                                        <NavLink to = {`/products/ ${product.id} `} className = \"btn btn-outline-dark\">Buy Now</NavLink>\n                                    </div>\n                                </div>\n\n                            </div>\n                        </>\n                    )\n                }) }\n            </>\n        )\n    }\n\n    return (\n        <div>\n            <div className=\"container my-5 py-5\">\n                <div className=\"row\">\n                    <div className=\"col-12 mb-5\">\n                        <h1 className=\"display-6 fw-bolder text-center\">Latest Products</h1> <hr/>  \n                    </div>\n                </div>\n                <div className=\"row justify-content-center\">{loading ? <Loading/> :<ShowProducts/>}</div>\n            </div>\n\n        </div>\n    )\n}\n\nexport default Products;","/home/dell/Documents/GitHub/ecommerce/src/redux/store.js",[],[],"/home/dell/Documents/GitHub/ecommerce/src/redux/reducer/index.js",[],[],"/home/dell/Documents/GitHub/ecommerce/src/redux/action/index.js",[],[],"/home/dell/Documents/GitHub/ecommerce/src/redux/reducer/handle