 <div className=\"col-md-6\">\n                    <img src={product.image} alt={product.title} height = \"400px\" width = \"400px\" />\n                </div>\n\n                <div className=\"col-md-6\">\n                    <h4 className=\"text-uppercase text-black-50\">{product.category}</h4>\n                    <h1 className=\"dispaly-5\">{product.title}</h1>\n                    <p className=\"lead fw-bolder\">\n                        Rating{product.rating && product.rating.rate}\n                        <i className=\"fa fa-star\"></i>\n                    </p>\n\n\n                    <h3 className=\"display-6 fw-bold my-4\">\n                        ${product.price}\n                    </h3>\n\n                    <p className=\"lead \">{product.description }</p>\n                \n                    <button className=\"btn btn-outline-dark px-4 py-2\" onClick={()=>addProduct(product)}>Add to Cart</button>\n                    <NavLink to=\"/cart\" className=\"btn btn-dark ms-2 px-3 py-2\">Go to Cart</NavLink>\n                </div>\n            </>\n        )\n\n    }\n\n    return (\n        <div>\n            <div className=\"container py-5\">\n                <div className=\"row py-4\">\n                    {loading ? <Loading/> :<ShowProduct/>}\n                </div>\n            </div>\n        </div>\n    );\n};\nexport default Product;","/home/dell/Documents/GitHub/ecommerce/src/component/About.jsx",[],[],"/home/dell/Documents/GitHub/ecommerce/src/component/Login_signup/Login.jsx",[],[],"/home/dell/Documents/GitHub/ecommerce/src/component/Register_page/Register.jsx",[],[],{"ruleId":"97","severity":1,"message":"98","line":4,"column":8,"nodeType":"99","messageId":"100","endLine":4,"endColumn":13},{"ruleId":"101","severity":1,"message":"102","line":25,"column":35,"nodeType":"103","endLine":25,"endColumn":40},{"ruleId":"104","severity":1,"message":"105","line":26,"column":13,"nodeType":"106","messageId":"107","endLine":26,"endColumn":19},{"rule